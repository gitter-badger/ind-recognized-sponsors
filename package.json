{
  "name": "github-pure-es6-client",
  "version": "0.1.0",
  "description": "This is the pure JavaScript ES6 implementation of github client",
  "scripts": {
    "start": "webpack-dev-server",
    "test": "mocha --compilers js:babel-core/register",
    "coverage": "./node_modules/babel-cli/bin/babel-node.js node_modules/.bin/babel-istanbul cover _mocha --report text --report html -- test",
    "coverage:watch": "./node_modules/babel-cli/bin/babel-node.js node_modules/.bin/babel-istanbul cover _mocha --report text --report html -- test",
    "test:watch": "mocha --compilers js:babel-core/register -w",
    "watch": "npm-watch"
  },
  "watch": {
    "coverage:watch": [
      "client/src/js/**/*.js",
      "test/**/*.js"
    ]
  },
  "author": "Ihor Pavlenko",
  "license": "unlicensed",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.4.0",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^6.1.2",
    "browser-sync": "^2.2.1",
    "browserify": "^8.1.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-spies": "^0.7.1",
    "css-loader": "^0.23.1",
    "exorcist": "^0.4.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^5.0.0",
    "gulp": "^3.8.11",
    "gulp-babel": "^6.1.2",
    "gulp-browserify": "^0.5.1",
    "gulp-jscs": "^3.0.2",
    "gulp-sass": "^2.3.1",
    "gulp-util": "^3.0.3",
    "gulp-watch": "^4.3.6",
    "gulp-webserver": "^0.9.1",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
    "node-sass": "^3.4.2",
    "npm-watch": "^0.1.5",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "https://github.com/substack/watchify",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "whatwg-fetch": "^1.0.0"
  }
}
